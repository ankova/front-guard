  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.2.4",
    "vitest": "^2.1.3"
  }
}
